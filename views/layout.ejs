		<div id="hello">
			<div id="titlebox">
				<img src="https://s3.amazonaws.com/lycheelabs/lychee.png" style="opacity: 0.95; width: 100px; float: left; margin-top: -10px">
				<div id="title"><b>lychee</b>labs</div>			
				<div id="subtitle"><%= title %></div>
			</div>
		</div>
		
		<%-body%>
		
		<div id="footer"></div>
